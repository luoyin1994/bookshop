html
  -webkit-tap-highlight-color rgba(0, 0, 0, 0)
  font-size 16px
  font-family "Microsoft Yahei"
ul
  margin 0
  padding 0

li
  list-style none

body, h1, h2, h3, h4, h5, h6, dl, dd, p
  margin 0

a
  cursor pointer
  display inline-block
  color #222222
  text-decoration none

span
  display inline-block

