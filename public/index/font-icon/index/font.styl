@import "variables"

@font-face
  font-family 'icomoon'
  src url(icomoon-font-path + '/icomoon.eot?9em3i8')
  src url(icomoon-font-path + '/icomoon.eot?9em3i8#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icomoon.ttf?9em3i8') format('truetype'), url(icomoon-font-path + '/icomoon.woff?9em3i8') format('woff'), url(icomoon-font-path + '/icomoon.svg?9em3i8#icomoon') format('svg')
  font-weight normal
  font-style normal

i
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-category
  &:before
    content icon-category
.icon-male
  &:before
    content icon-male
.icon-female
  &:before
    content icon-female
.icon-check
  &:before
    content icon-check
.icon-rank
  &:before
    content icon-rank
.icon-money
  &:before
    content icon-money
.icon-search
  &:before
    content icon-search
.icon-uncheck
  &:before
    content icon-uncheck
.icon-user
  &:before
    content icon-user

